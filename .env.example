# Planora.ai Environment Variables Template
# Copy this file to .env and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Application Configuration
VITE_APP_URL=http://localhost:5173
VITE_NODE_ENV=development

# Authentication Providers
VITE_ENABLE_GOOGLE_AUTH=true
VITE_ENABLE_APPLE_AUTH=true
VITE_ENABLE_EMAIL_AUTH=true

# OAuth Redirect URIs
VITE_GOOGLE_AUTH_REDIRECT_URI=http://localhost:5173/auth/callback
VITE_APPLE_AUTH_REDIRECT_URI=http://localhost:5173/auth/callback

# Production URLs (for deployment)
# VITE_APP_URL=https://getplanora.app
# VITE_GOOGLE_AUTH_REDIRECT_URI=https://getplanora.app/auth/callback
# VITE_APPLE_AUTH_REDIRECT_URI=https://getplanora.app/auth/callback

# Build Configuration
NODE_VERSION=20
